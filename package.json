{
  "name": "express",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon -r tsconfig-paths/register src/app.ts",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "db:check": "ts-node scripts/databaseConnectionChecker.ts",
    "db:init": "npx sequelize-cli db:create",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "mf": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
    "mfs": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "make:seeder": "node scripts/generateSeed.js",
    "seed:all": "npx sequelize-cli db:seed:all",
    "unseed:all": "npx sequelize-cli db:seed:undo",
    "make:secret": "node scripts/generateJWTSecret.js",
    "make:model": "node scripts/generateModel.js",
    "routes": "node scripts/checkRoutes.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.13.1",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mysql2": "^3.12.0",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/hpp": "^0.2.6",
    "@types/nodemailer": "^6.4.17",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "data-fns": "^1.1.0",
    "express-list-routes": "^1.2.4",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.10.0",
    "sequelize": "^6.37.5",
    "tsconfig-paths": "^4.2.0"
  }
}
